{
    "name": "sweet1s/moonshine-roles-permissions",
    "description": "Moonshine Role and Permission management",
    "keywords": ["laravel", "moonshine", "roles", "permissions"],
    "type": "library",
    "homepage": "https://moonshine.cutcode.dev",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/sweet1s/moonshine-roles-permissions/issues",
        "source": "https://github.com/sweet1s/moonshine-roles-permissions"
    },
    "authors": [
        {
            "name": "Andrei Balan",
            "email": "abalan1510it@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "ext-curl": "*",
        "ext-json": "*",
        "lee-to/laravel-package-command": "^0.9.1",
        "spatie/laravel-permission": "^5.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8",
        "mockery/mockery": "^1.4.4",
        "phpstan/phpstan": "^1.4.7",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0",
        "brianium/paratest": "^6.8"
    },
    "autoload": {
        "psr-4": {
            "Sweet1s\\MoonshineRolesPermissions\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sweet1s\\MoonshineRolesPermissions\\Providers\\MoonShineRolesPermissionsServiceProvider"
            ]
        }
    }
}
